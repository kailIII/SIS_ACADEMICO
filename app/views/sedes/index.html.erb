<h1>Listing sedes</h1>

<table>
  <thead>
    <tr>
      <th>Ciudad</th>
      <th>Facu carrera</th>
      <th>Universidad</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sedes.each do |sede| %>
      <tr>
        <td><%= sede.ciudad %></td>
        <td><%= sede.facu_carrera %></td>
        <td><%= sede.universidad %></td>
        <td><%= link_to 'Show', sede %></td>
        <td><%= link_to 'Edit', edit_sede_path(sede) %></td>
        <td><%= link_to 'Destroy', sede, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sede', new_sede_path %>
