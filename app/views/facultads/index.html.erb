<h1>Listing facultads</h1>

<table>
  <thead>
    <tr>
      <th>Nombre facu</th>
      <th>Direccion facu</th>
      <th>Telefono facu</th>
      <th>Email facu</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @facultads.each do |facultad| %>
      <tr>
        <td><%= facultad.nombre_facu %></td>
        <td><%= facultad.direccion_facu %></td>
        <td><%= facultad.telefono_facu %></td>
        <td><%= facultad.email_facu %></td>
        <td><%= link_to 'Show', facultad %></td>
        <td><%= link_to 'Edit', edit_facultad_path(facultad) %></td>
        <td><%= link_to 'Destroy', facultad, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Facultad', new_facultad_path %>
